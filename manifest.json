{
  "name": "websocket index",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "websocket mouse test",
  "browser_action": {
    "default_icon": "images/icon16.png"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["websocket.js"]
  },
  "permissions": [
    "tabs",
    "http://211.78.254.40:3008/*",
    "http://127.0.0.1:3008/*"
   ],
  "options_page": "options.html",
  "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "contentScript.js"
         ]
      }
  ]
}
